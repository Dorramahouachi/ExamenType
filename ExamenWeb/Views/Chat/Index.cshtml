            @Html.DisplayFor(modelItem => item.SendId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RecieveId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contenu)
        </td>

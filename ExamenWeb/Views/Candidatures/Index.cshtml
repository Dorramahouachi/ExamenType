@model IEnumerable<Domaine.Candidature>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutCustom.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Users.login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.candidatureDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.etat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Users.login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.candidatureDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.etat)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.candidatureId }) |
            @Html.ActionLink("Details", "Details", new { id=item.candidatureId }) |
        </td>
    </tr>
}

</table>
